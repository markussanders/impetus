{
  "manifest_version": 2,

  "name": "Impetus",
  "description": "This extension is a built to clone the popular Chrome Extension 'Momentum'.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "logo.png"
  },

  "chrome_url_overrides": {
    "newtab": "impetus.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "geolocation"
  ]
}